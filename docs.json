{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "Trunk",
  "colors": {
    "primary": "#09090b",
    "light": "#FAFAFA",
    "dark": "#09090b"
  },
  "favicon": "/favicon.svg",
  "appearance": {
    "default": "light",
    "strict": true
  },
  "navigation": {
    "tabs": [
      {
        "tab": "User Guide",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              {
                "group": "Quickstart",
                "pages": [
                  "dev/quickstart",
                  "dev/connect",
                  "dev/validator"
                ]
              },
              "dev/contribute"
            ]
          },
          {
            "group": "Concepts",
            "pages": [
              "concepts/network",
              "concepts/solvers",
              "concepts/routing",
              "concepts/privacy"
            ]
          },
          {
            "group": "Reference",
            "pages": [
              "appendix/approach",
              "appendix/properties",
              "appendix/changes"
            ]
          }
        ]
      }
    ],
    "global": {

      "anchors": [

        {
          "anchor": "Github",
          "href": "https://github.com/trunk",
          "icon": "github"
        },
        {
          "anchor": "Explorer",
          "href": "https://explorer.trunk.com",
          "icon": "magnifying-glass-waveform"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [],
    "primary": {
      "type": "button",
      "label": "GitHub",
      "href": "https://github.com/trunk"
    }
  },
  "seo": {
    "metatags": {
      "og:image": "https://raw.githubusercontent.com/trunk/docs/2eb6171ddbfeefde349dc3b8d5e2b87414c26250/images/og-image.png"
    },
    "indexing": "navigable"
  },
  "footer": {
    "socials": {
      "github": "https://github.com/trunk"
    }
  },
  "redirects": [
    {
      "source": "/quickstart",
      "destination": "/dev/quickstart"
    }
  ],
  "contextual": {
    "options": [
      "copy",
      "view"
    ]
  }
}